[sub_resource type="Shader" id=8]
code = "shader_type canvas_item;

uniform float scale = 1.0;
uniform vec4 color1 : hint_color;
uniform vec4 color2 : hint_color;

void fragment()
{
	float phi = -0.2*TIME*(10.0+0.0071*UV.x*(abs(cos(0.19*TIME))+0.25)-0.0091*UV.y*(abs(sin(0.17*TIME))+0.25));
	vec2 uvr = (UV-vec2(0.5))*mat2(vec2(cos(phi),sin(phi)),vec2(-sin(phi),cos(phi))) + vec2(0.5);
	vec2 uv = ((0.98*uvr+0.02*UV)-vec2(0.5))/scale + vec2(0.5);
	vec4 tex = texture(TEXTURE, uv);
	float w = tex.g*scale;
	COLOR = (1.0-w)*color1 + w*color2 + vec4(vec3(w*w),0.0);
	COLOR.a *= tex.a;
}"

