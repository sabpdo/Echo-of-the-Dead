[sub_resource type="Shader" id=16]
code = "shader_type canvas_item;
render_mode blend_add;

varying float offset;

void vertex()
{
//	vec2 v = (WORLD_MATRIX*vec4(VERTEX,1.0,1.0)).xy;
//	offset = sin(171.0*v.x)+cos(139.0*v.y);
	offset = sin(1.7*VERTEX.x)+cos(1.3*VERTEX.y);
}

void fragment()
{
	float pi = 3.1415926535798;
	float alpha = COLOR.a;
	vec2 uv = UV;
	uv.x += 0.2*sin(2.0*pi*(uv.y+1.7*TIME+offset));
	uv.y += 0.3*sin(2.0*pi*(0.5*uv.y-uv.x+1.3*TIME+offset));
	COLOR = texture(TEXTURE, uv);
	COLOR.a *= alpha*alpha;
}"

